{
  "version": 2,
  "name": "notebook",
  "builds": [
    {
      "src": "static/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/static/index.html"
    }
  ],
  "env": {
    "API_ENDPOINT": "https://your-render-backend.onrender.com"
  }
}
